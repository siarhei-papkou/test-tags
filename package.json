{
  "name": "test-tags-approach",
  "version": "2.0.3-beta.0",
  "description": "",
  "main": "index.js",
  "homepage": "https://www.npmjs.com/package/test-tags-approach",
  "scripts": {
    "dump-lib": "node ./scripts/dump-lib.js",
    "dump-lib:prerelease": "npm version prerelease --preid beta --force",
    "dump-lib:patch": "npm version patch --force",
    "dump-lib:minor": "npm run dump-lib -- --rank minor",
    "dump-lib:major": "npm run dump-lib -- --rank major",
    "commit-version": "node ./scripts/commit-version.js",
    "commit-version:patch": "npm run dump-lib:patch && npm run commit-version",
    "commit-version:minor": "npm run dump-lib:minor && npm run commit-version",
    "commit-version:major": "npm run dump-lib:major && npm run commit-version",
    "commit-version:prerelease": "npm run dump-lib:prerelease && npm run commit-version",
    "publish-lib": "node ./scripts/publish-lib.js",
    "publish-lib:dev": "node ./scripts/publish-lib.js --develop"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "node-cmd": "3.0.0",
    "np": "^7.6.0"
  }
}
